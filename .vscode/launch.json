  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke GitHub Action",
      "program": "${workspaceFolder}/node_modules/yo/lib/cli.js",
      "args": ["pcf:github-action"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke PCF RESX",
      "program": "${workspaceFolder}/node_modules/yo/lib/cli.js",
      "args": ["pcf:resx"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Invoke PCF Force",
      "program": "${workspaceFolder}/node_modules/yo/lib/cli.js",
      "args": [
        "pcf",
        "--ns ",
        "Fic",
        "--n",
        "SuperCoolControl",
        "--t",
        "field",
        "--pkg",
        "2",
        "--pp",
        "fic",
        "--pn",
        "IvanFicko"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
