{
  "name": "generator-pcf",
  "version": "1.4.7",
  "description": "Yeoman generator to kickstart your Power Apps Component Framework project.",
  "homepage": "",
  "author": {
    "name": "Ivan Ficko",
    "email": "ivan.ficko@outlook.com",
    "url": "https://dynamicsninja.blog"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "pcf",
    "power-apps",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^24.8.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chalk": "^2.1.0",
    "fs": "0.0.1-security",
    "jsonpath": "^1.0.2",
    "lodash": "^4.17.15",
    "xml2js": "^0.4.23",
    "yeoman-generator": "^2.0.1",
    "yo": "^3.1.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "DynamicsNinja/generator-pcf",
  "license": "MIT"
}
